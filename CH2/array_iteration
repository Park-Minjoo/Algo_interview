#C++
std:: string foo[] = {"A", "B", "C"};
for (std::string f : foo) {
    std::cout << f << std::endl;
}

// after version 11 -> modern C++
// : support

#JAVA
Sting[] foo = new String[]{"A", "B", "C"};
for (String f : foo) {
    System.out.println(f);
}

// for-each

#Python
foo = ['A', 'B', 'C']
for f in foo:
    print(f)

#Go
var foo = []string{"A", "B", "C"}
for _, f := rango foo {
    fmt.Println(f)
